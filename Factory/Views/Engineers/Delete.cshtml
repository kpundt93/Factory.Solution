@{
  Layout = "_Layout";
}

@model Factory.Models.Engineer;

<h1>Dr. Sillystringz's Factory</h1>
<hr />
<h2>Delete</h2>
<h3>Are you sure you want to delete this engineer?</h3>

<h4>Engineer: @Html.DisplayFor(model => model.FullName)</h4>

@using (Html.BeginForm())
{
  <button type="submit" value="Yes" class="btn btn-success"> Yes</button>
  <button class="btn btn-danger">@Html.ActionLink("No", "Details", new { id = Model.EngineerId })</button>
}

